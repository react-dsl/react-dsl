{
    "extends": "tslint:recommended",
    "rules": {
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "member-access": [
            true,
            "no-public"
        ],
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single"
        ]
    }
}
